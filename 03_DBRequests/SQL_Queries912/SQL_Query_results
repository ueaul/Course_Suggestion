SELECT MAX(semester) AS MaxExamSemesterAll
FROM pruefungsleistung

=> 20222



SELECT status AS statusBachelor, COUNT(*)
FROM studium
WHERE studium_bezeichnung = 'Wirtschaftsinformatik' and studium_art = 'Bachelor'
GROUP BY status;

B 	1
BE 	461
E 	100
N 	21
NB 	110
R 	559



